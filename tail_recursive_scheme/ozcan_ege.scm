(define (sonuncu list) (car (reverse list)))
(define (kirp liste) (cdr(reverse (cdr (reverse liste)))))

(define (foo list)
  (cond ((<= (length list) 1) #t)
				((not(equal? (car list) (sonuncu list))) #f)
        (else (foo (kirp list)))))

